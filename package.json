{
   "name": "boilerplate",
   "version": "1.0.0",
   "description": "Javascript boilerplate",
   "scripts": {
      "start": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\"",
      "backend:build": "node app.js",
      "backend:dev": "nodemon app.js ",
      "frontend:build": "webpack --progress --config webpack.config.build.js",
      "frontend:dev": "webpack serve --progress --config webpack.config.development.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Agarethx/boilerplate.git"
   },
   "author": "Cristian Quiroz",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Agarethx/boilerplate/issues"
   },
   "homepage": "https://github.com/Agarethx/boilerplate#readme",
   "devDependencies": {
      "@babel/core": "^7.18.9",
      "babel-loader": "^8.2.5",
      "clean-webpack-plugin": "^4.0.0",
      "concurrently": "^7.3.0",
      "copy-webpack-plugin": "^11.0.0",
      "css-loader": "^6.7.1",
      "eslint": "^8.20.0",
      "eslint-config-standard": "^17.0.0",
      "eslint-loader": "^4.0.2",
      "eslint-plugin-flowtype": "^8.0.3",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^6.0.0",
      "eslint-plugin-standard": "^5.0.0",
      "eslint-webpack-plugin": "^3.2.0",
      "file-loader": "^6.2.0",
      "glslify-loader": "^2.0.0",
      "image-minimizer-webpack-plugin": "^3.2.3",
      "imagemin": "^8.0.1",
      "imagemin-gifsicle": "^7.0.0",
      "imagemin-jpegtran": "^7.0.0",
      "imagemin-mozjpeg": "^10.0.0",
      "imagemin-optipng": "^8.0.0",
      "imagemin-pngquant": "^9.0.2",
      "imagemin-svgo": "^10.0.1",
      "mini-css-extract-plugin": "^2.6.1",
      "postcss-loader": "^7.0.1",
      "sass": "^1.54.0",
      "sass-loader": "^13.0.2",
      "terser-webpack-plugin": "^5.3.3",
      "webpack": "^5.73.0",
      "webpack-cli": "^4.10.0",
      "webpack-dev-server": "^4.9.3",
      "webpack-merge": "^5.8.0"
   },
   "dependencies": {
      "express": "^4.18.1",
      "nodemon": "^2.0.19",
      "pug": "^3.0.2"
   }
}
